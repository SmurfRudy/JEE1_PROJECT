		<title>Appel AJAX</title>
	</head>
	
	
	<body>
		
			<h:form>
				<h:inputText id="lastName" value="#{userBean.login}" /> 
				<f:ajax render="id1" event="keyup" execute="login"/>
				
				<h:inputText id="password" value="#{userBean.pwd}" /> 
				
				
			</h:form>
		<h:outputText id="id1" value="#{userBean.login}" />
		
	</body>
